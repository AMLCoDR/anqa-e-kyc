# Anqa OCR Service Configuration
REACT_APP_ANQA_OCR_URL=http://localhost:8080/api/ocr
REACT_APP_ANQA_API_KEY=your-anqa-ocr-api-key-here
REACT_APP_ANQA_TENANT=default

# OCR Processing Options
REACT_APP_OCR_CONFIDENCE_THRESHOLD=0.8
REACT_APP_OCR_LIVENESS_THRESHOLD=0.7
REACT_APP_OCR_FACE_QUALITY_THRESHOLD=0.6

# Document Processing
REACT_APP_SUPPORTED_DOCUMENT_TYPES=passport,driver_license,national_id,residence_permit
REACT_APP_MAX_IMAGE_SIZE=10485760
REACT_APP_SUPPORTED_IMAGE_FORMATS=jpeg,jpg,png

# API Endpoints
REACT_APP_API_BASE_URL=http://localhost:8080
REACT_APP_OCR_PROCESS_ENDPOINT=/api/ocr/process-document
REACT_APP_SELFIE_PROCESS_ENDPOINT=/api/ocr/process-selfie
